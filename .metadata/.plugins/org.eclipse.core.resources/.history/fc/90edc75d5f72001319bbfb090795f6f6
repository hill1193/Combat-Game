package Weapons;

public class Sword extends Weapon {
	public Sword(int attack, int defense, int speed, int stamina, String name) {
		this.attack = attack;
		this.defense = defense;
		this.speed = speed;
		this.stamina = stamina;
		this.name = name;
	}
	
	static final OneHand woodensword = new Sword(1, 0, 0, 0, "Wooden Sword");
	static final TwoHand woodenbroadsword = new Sword(2, 0, 0, 0, "Wooden Broadsword");
}
