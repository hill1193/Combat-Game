package Armor;

public abstract class Armor {
	int attack;
	int defense;
	int speed;
	int stamina;
	String name;
	
	int getAttack() {
		return this.attack;
	}
	
	int getDefense() {
		return this.defense;
	}
	
	int getSpeed() {
		return this.speed;
	}
	
	int getStamina() {
		return this.stamina;
	}
	
	String getName() {
		return this.name;
	}
	
}
